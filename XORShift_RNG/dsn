    %load/vec4 v000001dab44fc530_0;
    %load/vec4 v000001dab44fc530_0;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %shiftl 4;
    %xor;
    %assign/vec4 v000001dab44fc530_0, 0;
    %load/vec4 v000001dab44fc530_0;
    %addi 17, 0, 8;
    %assign/vec4 v000001dab44fc530_0, 0;
T_0.1 ;
